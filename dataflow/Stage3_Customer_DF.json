				"          Payment_Terms_Code as string,",
				"          Payment_Method_Code as string,",
				"          Collection_Method as string,",
				"          Customer_Scheme_Applicable as integer,",
				"          Customer_Contact as string,",
				"          Phone_No as string,",
				"          Phone_No_2 as string,",
				"          E_Mail as string,",
				"          PAN_NO as string,",
				"          TIN_NO as string,",
				"          Address as string,",
				"          Address_2 as string,",
				"          City as string,",
				"          State_Code as string,",
				"          Post_Code as string,",
				"          Customer_Salesperson_Code as string,",
				"          Customer_Executive_Code as string,",
				"          Customer_Route_Code as string,",
				"          Customer_Territory as string,",
				"          Customer_Status as integer,",
				"          Site_Code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'delta-lake',",
				"     folderPath: 'Master/Customer') ~> Stage3CustomerSource",
				"Stage3CustomerSource derive(Cust_Key = concat(toString(Site_Code),'|',Customer_No),",
				"          SP_Key = concat(toString(Site_Code),'|',Customer_Territory)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'primarysales',",
				"     tableName: 'dim_customer',",
				"     preSQLs:['DROP TABLE IF EXISTS [primarysales].[dim_customer];', 'CREATE TABLE [primarysales].[dim_customer] (    Cust_Key NVARCHAR(255) PRIMARY KEY NOT NULL,    SP_Key NVARCHAR(255) ,    Customer_No NVARCHAR(255) ,    Customer_Name NVARCHAR(255) ,    Customer_Category NVARCHAR(255) ,    Customer_Subcategory_Code NVARCHAR(255) ,    Customer_Group_Code NVARCHAR(255) ,    Customer_Parent_Group NVARCHAR(255) ,    Customer_Type NVARCHAR(255) ,    Billing_Type INT ,    Channel INT ,    Sub_Channel_1 NVARCHAR(255) ,    Sub_Channel_2 NVARCHAR(255) ,    Sub_Channel_3 NVARCHAR(255) ,    Creation_Date_Time DATETIME2 ,    Customer_Credit_Limit_Days INT ,    Credit_Limit_LCY DECIMAL ,    Customer_Posting_Group NVARCHAR(255) ,    Customer_General_Business_Posting_Group NVARCHAR(255) ,    Customer_Discount_Group NVARCHAR(255) ,    Customer_Price_Group NVARCHAR(255) ,    Currency_Code NVARCHAR(255) ,    Customer_Invoice_Discount_Code NVARCHAR(255) ,    Payment_Terms_Code NVARCHAR(255) ,    Payment_Method_Code NVARCHAR(255) ,    Collection_Method NVARCHAR(255) ,    Customer_Scheme_Applicable INT ,    Customer_Contact NVARCHAR(255) ,    Phone_No NVARCHAR(255) ,    Phone_No_2 NVARCHAR(255) ,    E_Mail NVARCHAR(255) ,    PAN_NO NVARCHAR(255) ,    TIN_NO NVARCHAR(255) ,    Address NVARCHAR(255) ,    Address_2 NVARCHAR(255) ,    City NVARCHAR(255) ,    State_Code NVARCHAR(255) ,    Post_Code NVARCHAR(255) ,    Customer_Salesperson_Code NVARCHAR(255) ,    Customer_Executive_Code NVARCHAR(255) ,    Customer_Route_Code NVARCHAR(255) ,    Customer_Territory NVARCHAR(255) ,    Customer_Status INT ,    Site_Code INT NOT NULL);'],",
				"     insertable: true,",
				"     updateable: false,",
				"     deletable: false,",
				"     upsertable: false,",
				"     recreate: true,",
				"     postSQLs:[' Alter table [primarysales].[dim_customer] \\n    Add Constraint Fk_CustKey  \\n    Foreign Key(SP_Key) References [primarysales].[dim_salesperson](S_Key)'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          Customer_No,",
				"          Customer_Name,",
				"          Customer_Category,",
				"          Customer_Subcategory_Code,",
				"          Customer_Group_Code,",
				"          Customer_Parent_Group,",
				"          Customer_Type,",
				"          Billing_Type,",
				"          Channel,",
				"          Sub_Channel_1,",
				"          Sub_Channel_2,",
				"          Sub_Channel_3,",
				"          Creation_Date_Time,",
				"          Customer_Credit_Limit_Days,",
				"          Credit_Limit_LCY,",
				"          Customer_Posting_Group,",
				"          Customer_General_Business_Posting_Group,",
				"          Customer_Discount_Group,",
				"          Customer_Price_Group,",
				"          Currency_Code,",
				"          Customer_Invoice_Discount_Code,",
				"          Payment_Terms_Code,",
				"          Payment_Method_Code,",
				"          Collection_Method,",
				"          Customer_Scheme_Applicable,",
				"          Customer_Contact,",
				"          Phone_No,",
				"          Phone_No_2,",
				"          E_Mail,",
				"          PAN_NO,",
				"          TIN_NO,",
				"          Address,",
				"          Address_2,",
				"          City,",
				"          State_Code,",
				"          Post_Code,",
				"          Customer_Salesperson_Code,",
				"          Customer_Executive_Code,",
				"          Customer_Route_Code,",
				"          Customer_Territory,",
				"          Customer_Status,",
				"          Site_Code,",
				"          Cust_Key,",
				"          SP_Key",
				"     )) ~> sink1"
			]
		}
	}
}