					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Customer_No as string,",
				"          Customer_Name as string,",
				"          Customer_Category as string,",
				"          Customer_Subcategory_Code as string,",
				"          Customer_Group_Code as string,",
				"          Customer_Parent_Group as string,",
				"          Customer_Type as string,",
				"          Billing_Type as integer,",
				"          Channel as integer,",
				"          Sub_Channel_1 as string,",
				"          Sub_Channel_2 as string,",
				"          Sub_Channel_3 as string,",
				"          Creation_Date_Time as timestamp,",
				"          Customer_Credit_Limit_Days as integer,",
				"          Credit_Limit_LCY as decimal(8,4),",
				"          Customer_Posting_Group as string,",
				"          Customer_General_Business_Posting_Group as string,",
				"          Customer_Discount_Group as string,",
				"          Customer_Price_Group as string,",
				"          Currency_Code as string,",
