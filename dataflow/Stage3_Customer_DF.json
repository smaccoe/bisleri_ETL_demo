{
	"name": "Stage3_Customer_DF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					},
					"name": "Stage3CustomerSource"
				}
			],
			"sinks": [
				{
