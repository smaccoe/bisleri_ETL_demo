				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true,",
				"     multiLineRow: true) ~> salesshipmentline",
				"salesshipmentline select(mapColumn(",
				"          {Document No_},",
				"          {Line No_},",
				"          Type,",
				"          {Sell-to Customer No_},",
				"          {Location Code},",
				"          Description,",
				"          {Unit of Measure},",
				"          Quantity,",
				"          {Unit Price},",
				"          {Unit Cost (LCY)},",
				"          {Shortcut Dimension 1 Code},",
				"          {Shortcut Dimension 2 Code},",
				"          {Quantity (Base)},",
				"          {Item Category Code},",
				"          {Free Supply},",
				"          {Unit Price Incl_ of Tax},",
				"          {GST Group Code},",
				"          {Total GST Amount},",
				"          {HSN_SAC Code},",
				"          {Scheme Reason},",
				"          {Scheme Code},",
				"          {Pack title},",
				"          {Scheme Parent Item No_},",
				"          ECOM,",
				"          {Last Modified On},",
				"          {Posting Date},",
				"          Site_Code",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> renamingcolums",
				"renamingcolums sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: ($Container_Name),",
				"     folderPath: ($Folder_Name),",
				"     mergeSchema: false,",
