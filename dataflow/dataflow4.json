{
	"name": "dataflow4",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Parquet6",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          No_ as string,",
				"          {Sell-toCustomerNo_} as string,",
				"          {Bill-toCustomerNo_} as string,",
				"          {Ship-toCode} as string,",
				"          {Ship-toName} as string,",
				"          {Ship-toAddress} as string,",
				"          LastModifiedOn as timestamp,",
				"          Site_Code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> source1",
				"source1 parse(format: 'json',",
				"     documentForm: 'documentPerLine') ~> parse1"
			]
		}
	}
}