{
	"name": "Stage2_Sales_Cr_Memo_Header_DF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Stage2_Sales_Cr_Memo_Header",
						"type": "DatasetReference"
					},
					"name": "SalesCrMemoHeader"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					},
					"name": "SalesCrMemoHeaderSink",
					"rejectedDataLinkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Container_Name as string ('curated-data'),",
				"     Folder_Name as string ('Sales Cr_Memo Header')",
				"}",
				"source(output(",
				"          No_ as string,",
				"          {Sell-to Customer No_} as string,",
				"          {Bill-to Customer No_} as string,",
				"          {Posting Date} as string,",
				"          {Shipment Date} as string,",
				"          {Location Code} as string,",
				"          ECOM as boolean,",
				"          {Last Modified On} as string,",
				"          Site_Code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SalesCrMemoHeader",
				"select1 derive(No_ = toString(iifNull(No_, \"None\", No_)),",
				"          Sell_to_Customer_No_ = toString(iifNull(Sell_to_Customer_No_, \"None\", Sell_to_Customer_No_)),",
				"          Bill_to_Customer_No_ = toString(iifNull(Bill_to_Customer_No_, \"None\", Bill_to_Customer_No_)),",
				"          Posting_Date = toTimestamp(toString(toTimestamp(Posting_Date,'yyyy-MM-dd HH:mm:ss','yyyy-MM-dd hh:mm:ss.SSS'),'yyyy-MM-dd hh:mm:ss.SSS')),",
				"          Shipment_Date = toTimestamp(toString(toTimestamp(Shipment_Date,'yyyy-MM-dd HH:mm:ss','yyyy-MM-dd hh:mm:ss.SSS'),'yyyy-MM-dd hh:mm:ss.SSS')),",
				"          Location_Code = toString(iifNull(Location_Code, \"None\", Location_Code)),",
				"          ECOM = iif(ECOM,1 ,0),",
				"          Last_Modified_On = toTimestamp(toString(toTimestamp(Last_Modified_On,'yyyy-MM-dd HH:mm:ss','yyyy-MM-dd hh:mm:ss.SSS'),'yyyy-MM-dd hh:mm:ss.SSS')),",
				"          Site_Code = toString(Site_Code)) ~> derivedColumn1",
				"SalesCrMemoHeader select(mapColumn(",
				"          No_,",
				"          Sell_to_Customer_No_ = {Sell-to Customer No_},",
				"          Bill_to_Customer_No_ = {Bill-to Customer No_},",
				"          Posting_Date = {Posting Date},",
				"          Shipment_Date = {Shipment Date},",
				"          Location_Code = {Location Code},",
				"          ECOM,",
				"          Last_Modified_On = {Last Modified On},",
				"          Site_Code",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'delta-lake',",
				"     folderPath: 'Transaction/Full-Load',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SalesCrMemoHeaderSink"
			]
		}
	}
}