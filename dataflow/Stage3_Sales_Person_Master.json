{
	"name": "Stage3_Sales_Person_Master",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					},
					"name": "salespersonmaster"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "DWH_Dump",
						"type": "LinkedServiceReference"
					},
					"name": "salespersonsink"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Sales_Person_Code as string,",
				"          Sales_Person_Name as string,",
				"          ASM_Code as integer,",
				"          Manager_Code as integer,",
				"          Executive_Code as integer,",
				"          SalesPerson_Location as string,",
				"          SalesPerson_Route as string,",
				"          Sales_Hierarchy_Code as string,",
				"          BranchCode_SP as string,",
				"          SalesPersonZone as string,",
				"          Site_Code as integer,",
				"          Sales_Executive_Name as string,",
				"          Sales_Manager_Name as string,",
				"          Area_Sales_Manager_Name as string,",
				"          Branch_Sales_Head_Name as string,",
				"          Regional_Sales_Head_Name as string,",
				"          Area_Code as string,",
				"          Area_Name as string,",
				"          Branch_Code as string,",
				"          Branch_Name as string,",
				"          Territory_Code as string,",
				"          Territory_Name as string,",
				"          Region_Code as string,",
				"          Region_Name as string,",
				"          Zone_Code as string,",
				"          Zone_Name as string,",
				"          National_Code as string,",
				"          National_Name as string,",
				"          Division_Name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'delta-lake',",
				"     folderPath: 'Master/Sales_Person_Master') ~> salespersonmaster",
				"salespersonmaster derive(S_Key = concat(toString(Site_Code),'|',Sales_Hierarchy_Code)) ~> derivedColumn1",
				"derivedColumn1 filter(isNull(S_Key)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'primarysales',",
				"     tableName: 'dim_salesperson',",
				"     preSQLs:['DROP TABLE IF EXISTS [primarysales].[dim_salesperson];', 'CREATE TABLE [primarysales].[dim_salesperson] (\\n    S_Key NVARCHAR(255) PRIMARY KEY NOT NULL,\\n    Sales_Person_Code NVARCHAR(255) ,\\n    Sales_Person_Name NVARCHAR(255) ,\\n    ASM_Code INT ,\\n    Manager_Code INT ,\\n    Executive_Code INT ,\\n    SalesPerson_Location NVARCHAR(255) ,\\n    SalesPerson_Route NVARCHAR(255) ,\\n    Sales_Hierarchy_Code NVARCHAR(255) ,\\n    BranchCode_SP NVARCHAR(255) ,\\n    SalesPersonZone NVARCHAR(255) ,\\n    Sales_Executive_Name NVARCHAR(255) ,\\n    Sales_Manager_Name NVARCHAR(255) ,\\n    Area_Sales_Manager_Name NVARCHAR(255) ,\\n    Branch_Sales_Head_Name NVARCHAR(255) ,\\n    Regional_Sales_Head_Name NVARCHAR(255) ,\\n    Area_Code NVARCHAR(255) ,\\n    Area_Name NVARCHAR(255) ,\\n    Branch_Code NVARCHAR(255) ,\\n    Branch_Name NVARCHAR(255) ,\\n    Territory_Code NVARCHAR(255) ,\\n    Territory_Name NVARCHAR(255) ,\\n    Region_Code NVARCHAR(255) ,\\n    Region_Name NVARCHAR(255) ,\\n    Zone_Code NVARCHAR(255) ,\\n    Zone_Name NVARCHAR(255) ,\\n    National_Code NVARCHAR(255) ,\\n    National_Name NVARCHAR(255) ,\\n    Division_Name NVARCHAR(255) ,\\n    Site_Code INT NOT NULL\\n);'],",
				"     insertable: true,",
				"     updateable: false,",
				"     deletable: false,",
				"     upsertable: false,",
				"     recreate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> salespersonsink"
			]
		}
	}
}