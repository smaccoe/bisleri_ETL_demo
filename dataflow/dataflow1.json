{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Parquet2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Code as string,",
				"          Name as string,",
				"          A_S_M as integer,",
				"          Agent as integer,",
				"          ARM as integer,",
				"          Location_Code as string,",
				"          Route as string,",
				"          Sales_hierarchy_code as string,",
				"          Global_Dimension_1_Code as string,",
				"          Zone as string,",
				"          Site_Code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> source1"
			]
		}
	}
}