{
	"name": "Stage2_Inc_Load_Sales_Shipment_Header_DF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Transaction_Full_Load_Folder",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "Delta_Full_Load_Folder",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     container_name as string ('delta-lake'),",
				"     folder_name as string ('Transaction/Incremental-Load/Sales_Shipment_Header')",
				"}",
				"source(output(",
				"          No_ as string,",
				"          {Sell-to Customer No_} as string,",
				"          {Bill-to Customer No_} as string,",
				"          {Ship-to Code} as string,",
				"          {Ship-to Name} as string,",
				"          {Ship-to Address} as string,",
				"          {Ship-to Address 2} as string,",
				"          {Ship-to City} as string,",
				"          {Posting Date} as string,",
				"          {Shipment Date} as string,",
				"          {Shipment Method Code} as string,",
				"          {Location Code} as string,",
				"          {Shortcut Dimension 1 Code} as string,",
				"          {Shortcut Dimension 2 Code} as string,",
				"          {Shipping Agent Code} as string,",
				"          {LR_RR No_} as string,",
				"          {LR_RR Date} as string,",
				"          {Vehicle No_} as string,",
				"          {Vehicle Type} as string,",
				"          {Route Code} as string,",
				"          {Customer Category} as string,",
				"          {Loading Slip No_} as string,",
				"          AutoPDASO as string,",
				"          ECOM as string,",
				"          {Last Modified On} as string,",
				"          {Salesperson Code} as string,",
				"          Site_Code as integer",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'data-lake',",
				"     folderPath: 'Transaction/Incremental-Load',",
				"     fileName: 'temp.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true,",
				"     multiLineRow: true) ~> source1",
				"source1 select(mapColumn(",
				"          Shipment_No = No_,",
				"          Customer_No = {Sell-to Customer No_},",
				"          Bill_to_Customer_No = {Bill-to Customer No_},",
				"          Ship_to_Code = {Ship-to Code},",
				"          Ship_to_Name = {Ship-to Name},",
				"          Ship_to_Address = {Ship-to Address},",
				"          Ship_to_Address_2 = {Ship-to Address 2},",
				"          Ship_to_City = {Ship-to City},",
				"          Posting_Date = {Posting Date},",
				"          Shipment_Date = {Shipment Date},",
				"          Shipment_Method_Code = {Shipment Method Code},",
				"          Location_Code = {Location Code},",
				"          Branch = {Shortcut Dimension 1 Code},",
				"          Division = {Shortcut Dimension 2 Code},",
				"          Shipping_Agent_Code = {Shipping Agent Code},",
				"          LR_RR_No = {LR_RR No_},",
				"          LR_RR_Date = {LR_RR Date},",
				"          Vehicle_No = {Vehicle No_},",
				"          Vehicle_Type = {Vehicle Type},",
				"          Route_Code = {Route Code},",
				"          Customer_Category = {Customer Category},",
				"          Loading_Slip_No = {Loading Slip No_},",
				"          AutoPDASO,",
				"          ECOM,",
				"          Last_Modified_On = {Last Modified On},",
				"          Site_Code,",
				"          Salesperson_Code = {Salesperson Code}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: ($container_name),",
				"     folderPath: ($folder_name),",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}