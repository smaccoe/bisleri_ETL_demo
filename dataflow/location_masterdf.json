{
	"name": "location_masterdf",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "location_master",
						"type": "DatasetReference"
					},
					"name": "locationmastersource"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "curated_data",
						"type": "LinkedServiceReference"
					},
					"name": "locationmastersink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          LocationCode as string,",
				"          LocationName as string,",
				"          LocationDepot as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> locationmastersource",
				"locationmastersource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'curated-data',",
				"     folderPath: 'Loaction master',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> locationmastersink"
			]
		}
	}
}