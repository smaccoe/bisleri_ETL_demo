				}
			],
			"scriptLines": [
				"parameters{",
				"     container_name as string ('delta-lake'),",
				"     folder_name as string ('Transaction/Full-Load/Sales_Shipment_Header')",
				"}",
				"source(output(",
				"          No_ as string,",
				"          {Sell-to Customer No_} as string,",
